@model WebApplication3.Models.User
@{
    ViewData["Title"] = "Home Page";
}
<!doctype html>
<html>
<head>
    <script>
        alert("Используя этот сайт, Вы даете согласие на использование cookies. На данном этапе Вы можете отказаться от использования cookies, настроив необходимые параметры в своем браузере.");
    </script>
</head>
<body>
    @using (Ajax.BeginForm("Auto", new AjaxOptions { UpdateTargetId = "results" }))
    {
        @Html.LabelFor(model => model.Login, "Логин")
        <br />
        @Html.EditorFor(model => model.Login)
        <br />
        <br />
        @Html.LabelFor(model => model.Password, "Пароль")
        <br />
        @Html.EditorFor(model => model.Password)
        <input type="submit" value="Войти" />
    }
    <td><p><a href="/Home/Reg">Зарегистрироваться</a></p></td>
</body>
</html>
